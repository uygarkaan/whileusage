public class Main {
    public static void main(String[] args) {
        //for
        for (int a = 2; a < 10; a += 2) {
            System.out.println(a);
        }
        System.out.println("for döngüsü tamamlandı.");


        int a = 1;
        //While
        while (a < 10) {
            System.out.println(a);
            a++;
        }
        System.out.println("while döngüsü tamamlandı.");
    }
}
